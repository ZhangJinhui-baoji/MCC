<?xml version="1.0" encoding="utf-8"?>
<group>
	<action shell="hidden">
		<title>powersave_thermal</title>
		<desc>省电模式温控</desc>
		<param
			name="powersave_thermal"
			title="对应模式触发温控限制的电池温度"
			desc="单位：摄氏度"
			type="seekbar"
			min="1"
			max="100"
			value-sh="sh ./kr-script/module_control/fas-rs/get_powersave_thermal.sh"/>
		<set>run set_powersave_thermal.sh</set>
		<summary sh="sh ./kr-script/module_control/fas-rs/get_powersave_thermal.sh" />
	</action>
