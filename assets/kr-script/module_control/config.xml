<?xml version="1.0" encoding="UTF-8" ?>
<resource dir="file:///android_asset/kr-script/module_control" />

    <group>
        <page
            logo="kr-script/icons/magisk.png"
            icon="kr-script/icons/magisk.png"
            id="asguard"
            config="asguard/config.xml"
            desc="无障碍服务守护" title="ASGuard"
            summary-sh="run path/asguard_path.sh">
            <option type="refresh" style="fab" icon="kr-script/icons/refresh.png">刷新</option>
        </page>
    </group>

    <group>
        <page
            logo="kr-script/icons/magisk.png"
            icon="kr-script/icons/magisk.png"
            id="uperf"
            config="uperf/config.xml"
            desc="用户空间性能控制器" title="UPERF"
            summary-sh="run path/uperf_path.sh">
            <option type="refresh" style="fab" icon="kr-script/icons/refresh.png">刷新</option>
        </page>
        <page
            logo="kr-script/icons/magisk.png"
            icon="kr-script/icons/magisk.png"
            id="dfps"
            config="dfps/config.xml"
            desc="动态屏幕刷新率控制器" title="DFPS"
            summary-sh="run path/dfps_path.sh">
            <option type="refresh" style="fab" icon="kr-script/icons/refresh.png">刷新</option>
        </page>
        <page
            logo="kr-script/icons/magisk.png"
            icon="kr-script/icons/magisk.png"
            id="qti"
            config="qti/config.xml"
            desc="内存管理优化" title="QTI"
            summary-sh="run path/qti_path.sh">
            <option type="refresh" style="fab" icon="kr-script/icons/refresh.png">刷新</option>
        </page>
    </group>

    <group>
        <page
            logo="kr-script/icons/magisk.png"
            icon="kr-script/icons/magisk.png"
            id="feas"
            config="feas/config.xml"
            desc="自动给配置指定的应用打开Feas，未指定的关闭" title="FEAS"
            summary-sh="run path/feas_path.sh">
            <option type="refresh" style="fab" icon="kr-script/icons/refresh.png">刷新</option>
        </page>
        <page
            logo="kr-script/icons/magisk.png"
            icon="kr-script/icons/magisk.png"
            id="fas-rs"
            config="fas-rs/config.xml"
            desc="通过某种方式监听帧变化，并且将此数据用于性能调度" title="FAS-RS"
            summary-sh="run path/fas-rs_path.sh">
            <option type="refresh" style="fab" icon="kr-script/icons/refresh.png">刷新</option>
        </page>
    </group>

    <group>
        <page
            logo="kr-script/icons/magisk.png"
            icon="kr-script/icons/magisk.png"
            id="QuantitativeStopCharging_switch"
            config="QuantitativeStopCharging_switch/config.xml"
            desc="到达指定电量、指定温度自动停止充电、自动恢复充电" title="QSC定量停充">
            <option type="refresh" style="fab" icon="kr-script/icons/refresh.png">刷新</option>
        </page>
        <page
            logo="kr-script/icons/magisk.png"
            icon="kr-script/icons/magisk.png"
            id="MiuiVariableThermal"
            config="MiuiVariableThermal/config.xml"
            desc="利用MIUI云温控，使充电场景、游戏场景用不同档位的快充和性能无限制，其它场景自动恢复默认温控，可配置MIUI旁路供电，可配置锁定屏幕刷新率" title="MIUI动态温控">
            <option type="refresh" style="fab" icon="kr-script/icons/refresh.png">刷新</option>
        </page>
    </group>

    <group>
        <page
            logo="kr-script/icons/magisk.png"
            icon="kr-script/icons/magisk.png"
            id="Bypass_Charge"
            config="Bypass_Charge/config.xml"
            desc="主板不再向手机电池充电" title="旁路充电">
            <option type="refresh" style="fab" icon="kr-script/icons/refresh.png">刷新</option>
        </page>
    </group>

    <group>
        <page
            config="about/config.xml"
            icon="kr-script/icons/mcc.png"
            desc=""
            title="关于软件">
        </page>
    </group>